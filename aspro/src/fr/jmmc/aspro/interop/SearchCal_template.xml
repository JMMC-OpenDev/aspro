<VOTABLE>
    <COOSYS ID="J2000" equinox="J2000." epoch="J2000." system="eq_FK5"/>
    <RESOURCE name="SearchCal from Aspro2">
        <TABLE>
            <PARAM datatype="string" name="band" value="_INSBAND_"/>
            <PARAM datatype="double" name="wlen"    value="_WAVELENGTH_" unit="um"/>
            <PARAM datatype="double" name="baseMax" value="_BASEMAX_" unit="m"/>

            <PARAM datatype="string" name="objectName" value="_TARGET-NAME_"/>
            <PARAM datatype="string" name="ra"  value="_RA_"  unit="HH:MM:SS.TT"/>
            <PARAM datatype="string" name="dec" value="_DEC_" unit="DD:MM:SS.TT"/>
            <PARAM datatype="double" name="mag" value="_MAG_"/>
            
<!-- TODO: remove following parameters as soon as SearchCal GUI 4.3.1 or 4.4 is released -->            

            <PARAM datatype="double" name="minMagRange" value="_MAG_MIN_"/>
            <PARAM datatype="double" name="maxMagRange" value="_MAG_MAX_"/>
            <PARAM datatype="boolean" name="bright" value="_BRIGHT_MODE_"/>

            <PARAM datatype="double" name="diffRa"  value="3600.0" unit="arcmin"/>
            <PARAM datatype="double" name="diffDec" value="1200.0" unit="arcmin"/>
            <PARAM datatype="double" name="radius"  value="10.0"   unit="arcmin"/>

            <PARAM datatype="string"  name="file"/>
            <PARAM datatype="double"  name="diamVK" unit="mas"/>
            <PARAM datatype="double"  name="visErr" value="0.5"/>
            <PARAM datatype="boolean" name="oldScenario" value="false"/>

            <DATA>
                <TABLEDATA>
                </TABLEDATA>
            </DATA>
        </TABLE>
    </RESOURCE>
</VOTABLE>
