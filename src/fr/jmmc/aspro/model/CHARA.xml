<!-- instruments (6T) -->

    <focalInstrument>
      <!-- name: Name of the focal instrument (AMBER, MIDI ...) -->
      <name>MIRC_6T</name>
      <!-- description: Description of the instrument -->
      <description>MIRC instrument using 6 telescopes (prototype)</description>
      <!-- numberChannels : The number of channels used by this instrument -->
      <numberChannels>6</numberChannels>
      <!-- defaultSamplingTime: The default sampling time for this instrument (min) -->
      <defaultSamplingTime>40</defaultSamplingTime>

      <!-- Noise modelling parameters -->
        <!-- The transmission of interferometer+instrument at observed wavelength (typically 0.01) -->
        <transmission>0.05</transmission>
        <!-- The Detector individual integration time in seconds (typically less than 0.1s) -->
        <dit>0.01</dit>
        <!-- ron: The Detector readout noise (typically 10) -->
        <ron>10</ron>
